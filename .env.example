# ElevenLabs API設定
ELEVENLABS_API_KEY=your_api_key_here
ELEVENLABS_BASE_URL=https://api.elevenlabs.io/v1
ELEVENLABS_TIMEOUT=30.0
ELEVENLABS_MAX_RETRIES=3

# アプリケーション設定
APP_ENV=development
API_HOST=0.0.0.0
API_PORT=8000

# データベース設定
DATABASE_URL=sqlite:///./app.db

# ロギング設定
LOG_LEVEL=INFO
LOG_FILE=

# キャッシュ設定
CACHE_ENABLED=true
CACHE_TTL=3600
CACHE_MAX_SIZE=1000

# ストレージ設定
STORAGE_PATH=./storage
STORAGE_AUDIO_DIR=audio
STORAGE_TEMP_DIR=temp